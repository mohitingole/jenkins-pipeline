pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'Hello build'
            }
        }
         stage('dev') {
            steps {
                echo 'Hello dev'
            }
        }
         stage('test') {
            steps {
                echo 'Hello test'
            }
        }
         stage('deploy') {
            steps {
                echo 'Hello deploy'
            }
        }
         stage('release') {
            steps {
                echo 'Hello release'
            }
        }
    }
}
